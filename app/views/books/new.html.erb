<h1>New Book</h1>

<%#= form_with model: @books do |form| %>
<%= form_with model: @book do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :photo %><br>
    <%= form.text_area :photo %>
  </div>

  <div>
    <%= form.label :describtion %><br>
    <%= form.text_area :describtion %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>